        "\t\t//map order variable to expected task contract input",
        "\t\torderInput: {",
        "\t\t\tdescription: $data.order.description !== undefined ? $data.order.description : null,",
        "\t\t\ttotalPrice: $data.order.totalPrice !== undefined ? $data.order.totalPrice : null,",
        "\t\t\ttotalTax: $data.order.totalTax !== undefined ? $data.order.totalTax : null,",
        "\t\t\torderStatus: $data.order.orderStatus !== undefined ? $data.order.orderStatus : null,",
